.listSelectIconWrap {
  position: absolute;
  width: 300rpx;
  height: 33px;
  top: 0;
  left: 225rpx;
}
.listSelectIcon {
  position: absolute;
  top: 9px;
  width: 12px;
  height: 12px;
}
.listSelectWrap {
  position: absolute;
  width: 90px;
  overflow: hidden;
  line-height: 35px;
  text-align: center;
  font-size: 14px;
  box-shadow: 0px 1px 6px 0px rgba(0, 0, 0, 0.1);
  border: 1px solid #dce0e7;
  z-index: 2;
}
.share-wrap {
  position: relative;
}
.share-wrap .rotation-chart {
  height: 330rpx;
  width: 100%;
}
.share-wrap .rotation-chart image {
  height: 100%;
  width: 100%;
}
.share-wrap .head-icon-background {
  height: 330rpx;
  position: relative;
  overflow: hidden;
}
.share-wrap .head-icon-background .vir-image {
  position: absolute;
  height: 750rpx;
  width: 750rpx;
  opacity: 0.3;
  filter: blur(10rpx);
  top: -195rpx;
}
.share-wrap .head-icon-background .rea-image {
  position: absolute;
  top: 140rpx;
  right: 0;
  height: 220rpx;
  width: 220rpx;
}
.share-wrap .share-message-remind {
  position: absolute;
  right: 24rpx;
  top: 324rpx;
  padding: 20rpx;
  z-index: 1;
}
.share-wrap .share-message-remind .remind-image {
  position: relative;
  width: 48rpx;
  height: 48rpx;
}
.share-wrap .share-message-remind .remind-red-dot {
  position: absolute;
  top: 24rpx;
  border-radius: 10rpx;
  width: 12rpx;
  height: 12rpx;
  background-color: red;
}
.share-wrap .chat-wrap {
  position: relative;
}
.share-wrap .chat-wrap .chat {
  margin-left: 24rpx;
  padding: 15rpx 0 15rpx 24rpx;
  border-left: 4rpx solid rgba(103, 167, 143, 0.7);
}
.share-wrap .chat-wrap .left-circle-icon {
  position: absolute;
  height: 24rpx;
  top: 20rpx;
  left: 15rpx;
  width: 24rpx;
}
.share-return-top-wrap {
  position: fixed;
  box-sizing: border-box;
  left: 668rpx;
  top: 1348rpx;
  width: 58rpx;
  height: 54rpx;
  line-height: 40rpx;
  border-radius: 8rpx;
  background-color: #8c8c8c;
  text-align: center;
  border: 2rpx solid #8c8c8c;
}
.share-return-top-wrap .image {
  position: absolute;
  top: 8rpx;
  left: 10rpx;
  width: 34rpx;
  height: 34rpx;
}
.collection-add-icon {
  position: fixed;
  width: 100rpx;
  height: 100rpx;
}
.menu-mask {
  position: absolute;
  right: 0;
  width: 750rpx;
  z-index: 11;
  background-color: rgba(0, 0, 0, 0.6);
  transition: all 0.2s ease;
}
